{
  "name": "mozzic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.8.3",
    "@craco/craco": "^6.1.2",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-actions": "^2.6.1",
    "axios": "^0.21.4",
    "concurrently": "^6.5.1",
    "craco-plugin-scoped-css": "^1.1.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "g": "^2.0.1",
    "jszip": "^3.7.1",
    "konva": "^8.3.2",
    "node-sass": "^6.0.0",
    "pdfjs-dist": "^2.10.377",
    "pspdfkit": "^2022.1.1",
    "react": "^16.14.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^16.8.0",
    "react-icons": "^4.3.1",
    "react-konva": "^17.0.2-5",
    "react-lottie": "^1.2.3",
    "react-lottie-hook": "^0.5.0",
    "react-movable": "^3.0.2",
    "react-pdf": "^5.6.0",
    "react-pdf-to-image": "^1.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "serve": "^13.0.2",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.3.2",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "env-cmd -f .env craco start",
    "build": "CI= react-scripts build",
    "test": "craco test",
    "eject": "craco eject",
    "server": "json-server --watch db.json --port 3001",
    "electron-start": "concurrently \"cross-env NODE_ENV=development BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build:mac": "npm run build && electron-builder --mac",
    "electron-build:win": "npm run build && electron-builder --win --x64",
    "release": "npm run build && electron-builder --publish=always",
    "preinstall": "npx npm-force-resolutions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "craco-alias": "^3.0.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "sass": "^1.43.5"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "author": "mozzic",
  "build": {
    "productName": "mozzic",
    "appId": "com.electron.mozzic",
    "asar": false,
    "mac": {
      "hardenedRuntime": true,
      "entitlements": "./build/entitlements.mac.inherit.plist",
      "target": [
        "default"
      ],
      "icon": "build/icon.png"
    },
    "protocols": {
      "name": "mozzic",
      "schemes": [
        "mozzic"
      ]
    },
    "asarUnpack": [
      "**/*.node"
    ],
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/installer/Icon.ico",
      "extraResources": [
        {
          "from": "app/dist/redistributable/",
          "to": "../",
          "filter": "*"
        }
      ]
    }
  }
}
